check process kinesis-to-s3
  with pidfile "/var/run/kinesis-to-s3.pid"
  start program = "/etc/init.d/kinesis-to-s3 start" with timeout 10 seconds
  stop program = "/etc/init.d/kinesis-to-s3 stop" with timeout 10 seconds
  if totalmem is greater than 1024 MB for 10 cycles then restart # eating up memory?